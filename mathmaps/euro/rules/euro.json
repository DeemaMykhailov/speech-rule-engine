{
  "locale": "euro",
  "modality": "braille",
  "domain": "default",
  "rules": [
    [
      "Rule",
      "stree",
      "default",
      "[n] ./*[1]",
      "self::stree"
    ],
    [
      "Rule",
      "latex",
      "default",
      "[n] @latex",
      "self::*[@latex]"
    ],
    [
      "Rule",
      "nolatex",
      "default",
      "[m] children/* (separator:\"⠀\")",
      "self::*[not(@latex)]", "name(.)!='stree'"
    ],
    [
      "Rule",
      "row",
      "default",
      "[m] children/* (separator:\"⠀⠯⠀\")",
      "self::row[not(@latex)]"
    ]
  ]
}
