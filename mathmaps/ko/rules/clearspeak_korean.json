{
  "domain": "clearspeak",
  "locale": "ko",
  "modality": "speech",
  "inherits": "base",
  "rules": [
    [
      "Precondition",
      "german-font",
      "default",
      "self::*[@font=\"fraktur\"]",
      "not(contains(@grammar, \"ignoreFont\"))"
    ],
    [
      "Precondition",
      "german-font-bold",
      "default",
      "self::*[@font=\"bold-fraktur\"]",
      "not(contains(@grammar, \"ignoreFont\"))"
    ],
    [
      "Precondition",
      "unit-square",
      "default",
      "self::superscript[@role=\"unit\"]",
      "children/*[2][text()=2]",
      "name(children/*[1])=\"identifier\"",
      "children/*[1][@category=\"unit:length\"]"
    ],
    [
      "Precondition",
      "unit-cubic",
      "default",
      "self::superscript[@role=\"unit\"]",
      "children/*[2][text()=3]",
      "name(children/*[1])=\"identifier\"",
      "children/*[1][@category=\"unit:length\"]"
    ],
    [
      "Precondition",
      "unit-quad",
      "default",
      "self::superscript[@role=\"unit\"]",
      "children/*[2][text()=4]",
      "name(children/*[1])=\"identifier\"",
      "children/*[1][@category=\"unit:length\"]"
    ],
    
    [
      "Precondition",
      "vbar-divides",
      "default",
      "self::punctuated",
      "@role=\"sequence\"",
      "count(./children/*)=3",
      "children/*[2][@role=\"vbar\"]"
    ],
    [
      "Precondition",
      "vbar-always-divides",
      "VerticalLine_Divides",
      "self::punctuated",
      "@role=\"sequence\"",
      "count(./children/*)=3",
      "children/*[2][@role=\"vbar\"]"
    ],
    [
      "Ignore",
      "equality"
    ],
    [
      "Precondition",
      "simple-relseq",
      "default",
      "self::relseq",
      "count(./children/*)=2",
      "text()=\"≥\" or text()=\">\" or text()=\"≤\" or text()=\"<\""
    ],
    [
      "Precondition",
      "set-member",
      "default",
      "self::infixop",
      "count(./children/*)=2",
      "text()=\"∈\" or text()=\"∊\""
    ],
    [
      "Precondition",
      "contain-member",
      "default",
      "self::infixop",
      "count(./children/*)=2",
      "text()=\"∋\" or text()=\"∍\"" 
    ],
    [
      "Precondition",
      "subset-of",
      "default",
      "self::relseq",
      "count(./children/*)=2",
      "text()=\"=\" or text()=\"<\" or text()=\">\" or text()=\"≤\" or text()=\"≥\" or text()=\"⊂\" or text()=\"≠\" or text()=\"⇐\" or text()=\"⇒\" or text()=\"⇔\""
    ],
    [
      "Precondition",
      "not-equal-to",
      "default",
      "self::relseq",
      "count(./children/*)=2",
      "text()=\"≠\""
    ],
    [
      "Precondition",
      "not-set-member",
      "default",
      "self::infixop",
      "count(./children/*)=2",
      "text()=\"∈\" or text()=\"∊\" or text()=\"∋\" or text()=\"∍\" or text()=\"∉\" or text()=\"∌\""
    ],
    [
      "Precondition",
      "parallel-to",
      "default",
      "self::punctuated",
      "count(./children/*)=3",
      "content/*[1][text()]=\"∥\""
    ],
    [
      "Precondition",
      "vertical",
      "default",
      "self::infixop",
      "count(./children/*)=2",
      "text()=\"⊥\""
    ],
    [
      "Precondition",
      "necessary-condition",
      "default",
      "self::relseq",
      "count(./children/*)=2",
      "text()=\"⇐\""
    ],
    [
      "Precondition",
      "sufficient-condition",
      "default",
      "self::relseq",
      "count(./children/*)=2",
      "text()=\"⇒\""
    ],
    [
      "Precondition",
      "necessary-sufficient-condition",
      "default",
      "self::relseq",
      "count(./children/*)=2",
      "text()=\"⇔\""
    ],
    [
      "Precondition",
      "line-segment",
      "default",
      "self::overscore",
      "@role=\"implicit\"",
      "children/*[2][@role=\"overaccent\"]",
      "children/*[2][contains(@annotation, \"accent:bar\")]",
      "name(children/*[1])=\"infixop\"",
      "count(./children/*[1]/children/*)=2"
    ]
  ]
}
