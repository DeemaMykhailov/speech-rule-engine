{
  "domain": "mathspeak",
  "locale": "it",
  "modality": "speech",
  "inherits": "romance",
  "rules": [
    [
      "Ignore",
      "vulgar-fraction"
    ],
    [
      "Precondition",
      "squared-male",
      "default",
      "self::number[text()=2]",
      "contains(@grammar, \"squared\")"
    ],
    [
      "Precondition",
      "squared-female",
      "default",
      "self::number[text()=2]",
      "contains(@grammar, \"squared\")",
      "parent::*/parent::*[@role=\"simple function\"]"
    ]
  ]
}
